        <property name="menus">
            <map>
                <entry>
                    <key>
                        <value>1</value>
                    </key>
                    <value>KingCrap</value>
                </entry>
                <entry>
                    <key>
                        <value>2</value>
                    </key>
                    <value>IceCream</value>
