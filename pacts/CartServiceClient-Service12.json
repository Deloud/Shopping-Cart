{
    "provider": {
        "name": "Service12"
    },
    "consumer": {
        "name": "CartServiceClient"
    },
    "interactions": [
        {
            "description": "A request for product information",
            "request": {
                "method": "GET",
                "path": "/1"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "id": 1,
                    "productlist": [
                        2
                    ],
                    "totalprice": 0
                },
                "matchingRules": {
                    "body": {
                        "$.totalprice": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.id": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.productlist": {
                            "matchers": [
                                {
                                    "match": "type",
                                    "min": 1
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.productlist[*]": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "Request cart information has id 1"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.10"
        }
    }
}